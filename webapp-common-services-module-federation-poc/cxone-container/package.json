{
  "name": "boilerplate",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --public-host na1.dev.localhost --port 5000",
    "build": "ng build --base-href /cxone-boilerplate/ --progress",
    "build-prod": "npm run test && ng build --configuration production --base-href /cxone-boilerplate/ --progress",
    "build-dev": "npm run lint && npm run test && npm run build",
    "test": "ng test --progress --watch=false",
    "lint": "eslint ./src --ext .js,.ts",
    "lint-fix": "eslint ./src --ext .js,.ts --fix",
    "page-objects": "copyfiles --flat \"src/**/*.po.js\" node_modules",
    "pree2e": "npm run page-objects",
    "e2e": "ng e2e --project=webapp-e2e --no-webdriver-update --suite",
    "e2e-suite_01": "npm run e2e suite_01",
    "e2e-suite_11": "npx playwright install chromium && npx playwright test --config=e2e/suite01/suite01.config.ts",
    "e2e-suite_12": "npx playwright install chromium && npx playwright test --config=e2e/suite02/suite02.config.ts",
    "playwright1": "npx playwright test --config=e2e/suite01/suite01.config.ts",
    "playwright2": "npx playwright test --config=e2e/suite02/suite02.config.ts",
    "e2e-cucumer-suite": "TS_NODE_PROJECT='./e2e/cucumber/tsconfig.commonjs.json' npx cucumber-js e2e/cucumber/features/*"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/all-modules": "~25.3.0",
